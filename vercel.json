{
  "version": 2,
  "functions": {
    "build/server/index.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/build/client/assets/$1"
    },
    {
      "src": "/favicon.ico", 
      "dest": "/build/client/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/build/server/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
